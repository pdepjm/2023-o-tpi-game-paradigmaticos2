import wollok.game.*
import clases.*

program abc {
	const matias = new Matias()
	
	//game.addVisual(celda)
	game.addVisual(celdadir)
	game.onCollideDo(celdadir, {obj => obj.direccion( celdadir.direccion() ) } )
	game.onTick(2000, "moverEnemigo",  {matias.moverse()})
	game.addVisual(matias)
	game.start()
}
